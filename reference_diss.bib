@Article{Sinha1992,
  Title                    = {{A Contact Stress Model for Multifingered Grasps of Rough Objects}},
  Author                   = {Sinha, Pramath Raj and Abel, Jacob M.},
  Journal                  = {IEEE Transactions on Robotics and Automation},
  Year                     = {1992},
  Number                   = {1},
  Pages                    = {7--22},
  Volume                   = {8},

  Abstract                 = {A model that utilizes a contact-stress analysis of an arbitrarily$\backslash$nshaped object in a multifingered grasp is developed. The fingers and the$\backslash$nobject are all treated as elastic bodies, and the region of contact is$\backslash$nmodeled as a deformable surface patch. The relationship between the$\backslash$nfriction and normal forces is now nonlocal and nonlinear in nature and$\backslash$ndeparts from the Coulomb approximation. The nature of the constraints$\backslash$narising out of conditions for compatibility and static equilibrium$\backslash$nmotivated the formulation of the model as a nonlinear constrained$\backslash$nminimization problem. The model is able to predict the magnitude of the$\backslash$ninwardly directed normal forces and both the magnitude and direction of$\backslash$nthe tangential (friction) forces at each finger/object interface for$\backslash$ngrasped objects in static equilibrium. Examples in two and three$\backslash$ndimensions are presented along with an application of the model to the$\backslash$ngrasp transfer maneuver},
  Doi                      = {10.1109/70.127235},
  ISBN                     = {0-8186-9061-5},
  ISSN                     = {1042296X}
}

@Article{Skubic2000,
  Title                    = {{Acquiring robust, force-based assembly skills from human demonstration}},
  Author                   = {Skubic, Marjorie and Volz, Richard A.},
  Journal                  = {IEEE Transactions on Robotics and Automation},
  Year                     = {2000},
  Number                   = {6},
  Pages                    = {772--781},
  Volume                   = {16},

  Abstract                 = {Robots have been used successfully in structured settings, where$\backslash$nthe environment is controlled; this research is inspired by the vision$\backslash$nof robots moving beyond structured, controlled settings. The work$\backslash$nfocuses on the problem of teaching robots force-based assembly skills$\backslash$nfrom human demonstration. To avoid position dependencies, force-based$\backslash$ndiscrete states (contact formations) are used to describe qualitatively$\backslash$nhow contact is being made with the environment. Sensorimotor skills are$\backslash$nmodeled using a hybrid control model, which provides a mechanism for$\backslash$ncombining continuous low-level force control with higher-level discrete$\backslash$nevent control. A change in qualitative, discrete state constitutes an$\backslash$nevent and triggers a new control command to the robot, which moves the$\backslash$nassembly toward a new contact formation. In this way, the skill$\backslash$nexecution is not dependent on absolute position but rather responds to$\backslash$nchanges in the force-based qualitative state. Experimental results are$\backslash$npresented which validate the approach and show how skill acquisition can$\backslash$nbe accomplished even with an imperfect demonstration},
  Doi                      = {10.1109/70.897788},
  ISSN                     = {1042296X}
}

