@Article{Tegin2009,
  Title                    = {{Demonstration-based learning and control for automatic grasping}},
  Author                   = {Tegin, Johan and Ekvall, Staffan and Kragic, Danica and Wikander, Jan and Iliev, Boyko},
  Journal                  = {Intelligent Service Robotics},
