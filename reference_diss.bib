@Article{Mosemann2001,
  Title                    = {{Automatic decomposition of planned assembly sequences into skill primitives}},
  Author                   = {Mosemann, H. and Wahl, F. M.},
  Journal                  = {IEEE Transactions on Robotics and Automation},
